<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.sagar.eschool.Main2Activity">

    <TextView
        android:text="Attendence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton7"
        android:layout_alignParentStart="true"
        android:layout_marginTop="12dp"
        android:id="@+id/textView7"
        android:textStyle="normal|bold"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        android:layout_toStartOf="@+id/imageView2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/profile1"
        android:id="@+id/imageButton6"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:scaleType="fitCenter"
        android:onClick="profile"
        />

    <TextView
        android:text="Time Table"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:textStyle="normal|bold"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignParentEnd="true"
        />

    <TextView
        android:text="Result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignStart="@+id/imageButton10"
        android:id="@+id/textView8"
        android:textStyle="normal|bold"
        android:textColor="@android:color/holo_red_dark"
        android:layout_alignParentEnd="true"
        android:textAlignment="center"
        android:textSize="18sp"
       />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/notice"
        android:id="@+id/imageButton11"
        android:onClick="notification"
        android:layout_above="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"/>

    <TextView
        android:text="Notice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:id="@+id/textView9"

        android:layout_alignStart="@+id/imageButton11"
        android:layout_alignEnd="@+id/imageButton11"
        android:textStyle="normal|bold"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        android:textAlignment="center" />



    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/attendence"
        android:id="@+id/imageButton7"
        android:layout_below="@+id/textView5"
        android:layout_alignParentStart="true"
        android:scaleType="fitCenter"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/calender"
        android:layout_alignStart="@+id/textView8"
        android:id="@+id/imageButton14"
        android:layout_alignBottom="@+id/imageButton13"
        android:layout_alignEnd="@+id/imageButton10"
        android:layout_below="@+id/textView8"
        android:scaleType="fitCenter"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/call"
        android:layout_alignTop="@+id/imageButton14"
        android:layout_alignParentStart="true"
        android:id="@+id/imageButton15"
        android:layout_above="@+id/textView11"
        android:layout_alignEnd="@+id/textView7"
        android:scaleType="fitCenter"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/exam"
        android:id="@+id/imageButton13"
        android:scaleType="fitCenter"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView9" />

    <TextView
        android:text="Call School"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        android:layout_below="@+id/imageButton15"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Exam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        android:layout_below="@+id/imageButton13"
        android:layout_alignStart="@+id/imageButton13"
         />

    <TextView
        android:text="Calender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        android:layout_below="@+id/imageButton14"
        android:layout_alignStart="@+id/imageButton14"
        android:layout_alignEnd="@+id/imageButton19" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/result"
        android:onClick="student_result"
        android:id="@+id/imageButton10"
        android:layout_below="@+id/textView6"
        android:layout_alignEnd="@+id/imageButton19"
        android:scaleType="fitCenter"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/timetable1"
        android:id="@+id/imageButton19"
        android:layout_above="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView2"
        android:layout_below="@+id/imageView2"
        android:scaleType="fitCenter"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logo"
        android:id="@+id/imageView2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"/>

    <TextView
        android:text="Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:textStyle="normal|bold"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp"
        android:textAlignment="center"
        android:layout_below="@+id/imageButton6"
        android:layout_marginEnd="45dp"
        android:layout_alignParentStart="true" />

</RelativeLayout>
